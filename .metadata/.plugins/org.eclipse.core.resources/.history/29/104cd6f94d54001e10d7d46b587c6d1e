package AllCollection.Queue;

import java.util.*;


public class CreatePQ {

	public static void main(String[] args) {
		Queue pq=new PriorityQueue();
		pq.add(100);
		pq.offer(100);
		pq.add(1234);
		pq.offer(101001);
//		pq.add(null);
//		pq.add(null);
//		pq.offer("Siva");
//		pq.add(1234.5678);
//		pq.offer('S');
//		pq.add(true);
//		pq.add(null);
//		pq.offer(null);
//		pq.add(1234.5678);
//		pq.offer('S');
		
		Queue p=new PriorityQueue();
		p.add(1000);
		p.offer(100000);
		p.add(9765);
		p.offer(12345678);
		
		pq.addAll(p);
		System.out.println(pq);
		
		//-------------------------
		
		//Retriving data 
		
		
		System.out.println(pq.element());//100
		System.out.println(pq.peek());//100
	
	}

}
