package MapInterface.TreeMap;

import java.util.*;


public class CreateHashTable {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Hashtable s=new Hashtable();
		
		s.put(1,"100");
		s.put(4,'S');
		s.put(2, 1.34f);
		s.put(5,true);
		s.put(6,123456);
		s.put(8,"100");
		s.put('a',null);
		s.put(2.44,null);
		s.put(10,false);
		s.put("Okkkk","0k");
		s.put(true,"200");
		//s.put(null,"0k");//NPE
		
		System.out.println(s);
		
			
			
			
		

	}

}
