package AllCollection.SetInterface;

import java.util.HashSet;
import java.util.Set;

public class Hashset {

	public static void main(String[] args) {
		
//		Set a=new HashSet();
		HashSet a=new HashSet();
		
		
		//adding
		
		a.add(100);
		a.add("Siva");
		a.add('S');
		a.add(100);
		a.add(null);
		a.add(12.3456);
		a.add(100);
		a.add(null);
		
		System.out.println(a);
		
		Set s=new HashSet();
		
		s.add(200);
		s.add("Reddy");
		s.add(true);
		s.add(null);
		s.add(1234.1234);
		
		a.addAll(s);
		System.out.println(a);
	}

}
