package AllCollection.Queue;

import java.util.*;


public class CreateADQ {

	public static void main(String[] args) {
		Deque adq=new ArrayDeque();
		
		
		//adding
		adq.add(100);
		adq.addFirst(1234);
		adq.addLast(1234.5678);
		
		adq.offer(100);
		adq.offerLast(true);
		adq.offerFirst("Siva");
		
		
		System.out.println(adq);
		
		Deque a=new ArrayDeque();
		
		a.offer('S');
		a.add(true);
		a.add(1234.5678);
		a.offer('S');
		
		adq.addAll(a);
		System.out.println(adq);
		
		
		
		
		
		
		
	
	}

}
