package AllCollection.Arraylist;

import java.util.ArrayList;


public class LoopingToAL {

	public static void main(String[] args) {
		
		ArrayList al=new ArrayList();
		
		al.add(100);
		al.add("siva");
		al.add(20.4);
		al.add('d');

		//for loop
		
		for(int i=0;i<=al.size()-1;i++) {
			System.out.println(al.get(i));
		}
		
		
		
		
	}

}
