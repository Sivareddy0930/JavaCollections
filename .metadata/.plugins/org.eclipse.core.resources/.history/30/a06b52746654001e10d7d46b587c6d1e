package AllCollection.Queue;

import java.util.*;


public class LoopingThroughADQ {

	public static void main(String[] args) {
		Deque adq=new ArrayDeque();
		
		
		//adding
		adq.add(100);
		adq.addFirst(1234);
		adq.addLast(1234.5678);
		
		adq.offer(100);
		adq.offerLast(true);
		adq.offerFirst("Siva");
		
		
		for(int i=0;i<adq.size();i++) {
			System.out.println(adq);
		}
		
		
		
		
		
		
		
		
		
		
		
		
	
	}

}
